{% set sectionno = '0' %}

{% for item in post.get_field('stroken') %}

	{% set sectionno = sectionno + '1' %}
	
	{% if item.acf_fc_layout == 'content' %}
		{% include 'pagebuilder/content.twig' %}
	
	{% elseif item.acf_fc_layout == 'blog' %}
		{% include 'pagebuilder/blog.twig' %}
	
	{% elseif item.acf_fc_layout == 'cta' %}
		{% include 'pagebuilder/cta.twig' %}
	
	{% elseif item.acf_fc_layout == 'accordion' %}
		{% include 'pagebuilder/accordion.twig' %}
		
	{% elseif item.acf_fc_layout == 'statusbar' %}
		{% include 'pagebuilder/statusbar.twig' %}
	
	{% elseif item.acf_fc_layout == 'video' %}
		{% include 'pagebuilder/video.twig' %}
	
	{% elseif item.acf_fc_layout == 'afbeelding' %}
		{% include 'pagebuilder/afbeelding.twig' %}
	
	{% elseif item.acf_fc_layout == 'blokken' %}
		{% include 'pagebuilder/blokken.twig' %}
	
	{% endif %}
  
{% endfor %}